{
  "name": "remix-shows",
  "private": true,
  "description": "TV Show guide built with Remix",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "cross-env NODE_ENV=development netlify dev",
    "start": "cross-env NODE_ENV=production netlify dev",
    "prepare": "husky install",
    "lint": "eslint --max-warnings 0 .",
    "lint:fix": "eslint --fix --max-warnings 0 .",
    "format": "prettier --check --ignore-unknown .",
    "format:fix": "prettier --write --ignore-unknown .",
    "codequality": "npm run format && npm run lint  && npm run typecheck",
    "codequality:fix": "npm run format:fix && npm run lint:fix && npm run typecheck",
    "typecheck": "tsc",
    "updates": "npm-check-updates -i"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.2.4",
    "@chakra-ui/react": "^2.8.1",
    "@emotion/react": "^11.13.3",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.13.0",
    "@netlify/functions": "^1.6.0",
    "@remix-run/netlify": "^1.19.3",
    "@remix-run/node": "^1.16.0",
    "@remix-run/react": "^1.16.0",
    "@types/is-touch-device": "^1.0.3",
    "cross-env": "^7.0.3",
    "framer-motion": "^10.16.4",
    "go-try": "^3.0.2",
    "http-errors": "^2.0.0",
    "is-touch-device": "^1.0.1",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-infinite-scroll-hook": "^5.0.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.16.0",
    "@remix-run/eslint-config": "^1.16.0",
    "@types/http-errors": "^2.0.4",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.10",
    "netlify-cli": "^13.0.1",
    "npm-check-updates": "^17.1.11",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=14"
  }
}
