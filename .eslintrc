{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "@remix-run/eslint-config",
    "@remix-run/eslint-config/node",
    "prettier"
  ],
  "rules": {
    "no-console": "warn",
    "no-alert": "warn",
    "prefer-destructuring": "warn",
    "prefer-const": "warn",
    "object-shorthand": "warn",
    "curly": "warn",
    "eqeqeq": "warn",
    "require-await": "warn"
  }
}
